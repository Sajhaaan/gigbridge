{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["./app/components/*"],
      "@screens/*": ["./app/screens/*"],
      "@navigation/*": ["./app/navigation/*"],
      "@assets/*": ["./assets/*"],
      "@utils/*": ["./app/utils/*"],
      "@hooks/*": ["./app/hooks/*"],
      "@types/*": ["./app/types/*"],
      "@constants/*": ["./app/constants/*"],
      "@services/*": ["./app/services/*"],
      "@context/*": ["./app/context/*"],
      "@providers/*": ["./app/providers/*"]
    },
    "esModuleInterop": true,
    "jsx": "react-native"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
